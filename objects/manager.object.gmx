          <argument>
            <kind>1</kind>
            <string>//reset shaders
for(row = 0; row &lt; (array_length_2d(global.level, 0))-1; row++){
    for(col = 0; col &lt; (array_height_2d(global.level))-1; col++){
        shaderMap[col, row].reset = true;
    }
}

//deco code
for(col = 0; col &lt; (room_width/16); col++ ){
    for(row = 0; row &lt; (room_height/16); row++ ){
        //sets in array coord values
        if(col-1 &lt; room_width/16 &amp;&amp; col-1 &gt;=0) {up = col-1}
        else {up = col}
        if(col+1 &lt; room_width/16 &amp;&amp; col+1 &gt;=0) {down = col+1}
        else {down = col}
        if(row-1 &lt; room_height/16 &amp;&amp; row-1 &gt;=0) {left = row-1}
        else {left = row}
        if(row+1 &lt; room_height/16 &amp;&amp; row+1 &gt;=0) {right = row+1}
        else {right = row}
        //places torches (temp?)    
        if(global.level[col,row].type == "wall" 
        &amp;&amp; global.level[up,left].type == "empty"
        &amp;&amp; global.level[up,right].type == "empty"
        &amp;&amp; global.level[up,row].type == "empty"
        &amp;&amp; global.level[down,right].type == "empty"
        &amp;&amp; global.level[down,left].type == "empty"
        &amp;&amp; global.level[down,row].type == "empty"
        &amp;&amp; global.level[col,left].type == "empty"
        &amp;&amp; global.level[col,right].type == "empty")
        {
            global.level[col,row].makeTorch = true
        }
        if(global.level[col,row].type == "wall" 
        &amp;&amp; (global.level[up,left].type == "empty"
        || global.level[up,right].type == "empty"
        || global.level[up,row].type == "empty"
        || global.level[down,right].type == "empty"
        || global.level[down,left].type == "empty"
        || global.level[down,row].type == "empty"
        || global.level[col,left].type == "empty"
        || global.level[col,right].type == "empty"))
        {
        tile_add(stoneWallTileset, irandom_range(0,3)*16, irandom_range(0,3)*16, 16, 16, col*16, row*16, 0)
        if(action_if_dice(10))
        {
        tile_add(vineTileset, irandom_range(0,3)*16, irandom_range(0,3)*16, 16, 16, col*16, row*16, -1)       
        }
        }
        /*if(global.level[col,row].type == "wall" 
        &amp;&amp; global.level[up,left].type == "wall"
        &amp;&amp; global.level[up,right].type == "wall"
        &amp;&amp; global.level[up,row].type == "wall"
        &amp;&amp; global.level[down,right].type == "wall"
        &amp;&amp; global.level[down,left].type == "wall"
        &amp;&amp; global.level[down,row].type == "wall"
        &amp;&amp; global.level[col,left].type == "wall"
        &amp;&amp; global.level[col,right].type == "wall")
        {
        tile_add(shadingTileset, 96, 0, 16, 16, col*16, row*16, 0)
        }
        */
        
    }
}


for(row = 1; row &lt; (array_length_2d(global.level, 0))-1; row++){
    for(col = 1; col &lt; (array_height_2d(global.level))-1; col++){
        
        if(global.level[col, row].type == "empty"){
            
            right = global.level[col+1, row].type != "empty";
            left = global.level[col-1, row].type != "empty";
            top = global.level[col, row-1].type != "empty";
            bottom = global.level[col, row+1].type != "empty";
            
            top_right = global.level[col+1, row-1].type != "empty";
            top_left = global.level[col-1, row-1].type != "empty";
            bottom_right = global.level[col+1, row+1].type != "empty";
            bottom_left = global.level[col-1, row+1].type != "empty";
            
            shaderMap[col,row].empty = 1;
            
            if(right)
                shaderMap[col+1,row].left+=2;
            if(left)
                shaderMap[col-1,row].right+=2;
            if(top)
                shaderMap[col,row-1].bottom+=2;
            if(bottom)
                shaderMap[col,row+1].top+=2;
            
            
            if(top_right &amp;&amp; top &amp;&amp; right){
                shaderMap[col+1,row-1].left++;
                shaderMap[col+1,row-1].bottom++;                
                shaderMap[col+1,row-1].inner++;
                shaderMap[col+1,row-1].top_right++;//this is to differentiate the final 2 shading types, which are completly similar in LRUD metrics
            }
            if(top_left &amp;&amp; top &amp;&amp; left){
                shaderMap[col-1,row-1].right++;
                shaderMap[col-1,row-1].bottom++;
                shaderMap[col-1,row-1].inner++;
            }
            if(bottom_right &amp;&amp; bottom &amp;&amp; right){
                shaderMap[col+1,row+1].left++;
                shaderMap[col+1,row+1].top++;
                shaderMap[col+1,row+1].inner++;
            }
            if(bottom_left &amp;&amp; bottom &amp;&amp; left){
                shaderMap[col-1,row+1].right++;
                shaderMap[col-1,row+1].top++;
                shaderMap[col-1,row+1].inner++;
            }         
        }
    }
}

//update shaders
for(row = 0; row &lt; (array_length_2d(global.level, 0))-1; row++){
    for(col = 0; col &lt; (array_height_2d(global.level))-1; col++){
        shaderMap[col,row].update = true;
    }
}


