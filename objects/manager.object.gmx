            <string>//Decoration Code
for(col = 1; col &lt; (room_width/16)-1; col++ ){
    for(row = 1; row &lt; (room_height/16)-1; row++ ){
        //Torches
        if(global.level[col,row].type == "wall" 
        &amp;&amp; global.level[col-1,row+1].type == "empty"
        &amp;&amp; global.level[col-1,row-1].type == "empty"
        &amp;&amp; global.level[col-1,row].type == "empty"
        &amp;&amp; global.level[col+1,row+1].type == "empty"
        &amp;&amp; global.level[col+1,row-1].type == "empty"
        &amp;&amp; global.level[col+1,row].type == "empty"
        &amp;&amp; global.level[col,row-1].type == "empty"
        &amp;&amp; global.level[col,row+1].type == "empty")
        {
            global.level[col,row].makeTorch = true
        }
        
        
    }
}

for(row = 1; row &lt; (array_length_2d(global.level, 0))-1; row++){
    for(col = 1; col &lt; (array_height_2d(global.level))-1; col++){
        
        if(global.level[col, row].type == "empty"){
            
            right = global.level[col+1, row].type != "empty";
            left = global.level[col-1, row].type != "empty";
            top = global.level[col, row-1].type != "empty";
            bottom = global.level[col, row+1].type != "empty";
            
            top_right = global.level[col+1, row-1].type != "empty";
            top_left = global.level[col-1, row-1].type != "empty";
            bottom_right = global.level[col+1, row+1].type != "empty";
            bottom_left = global.level[col-1, row+1].type != "empty";
            
            shaderMap[col,row].empty = 1;
            
            if(right)
                shaderMap[col+1,row].left++;
            if(left)
                shaderMap[col-1,row].right++;
            if(top)
                shaderMap[col,row-1].bottom++;
            if(bottom)
                shaderMap[col,row+1].top++;
            
            
            if(top_right &amp;&amp; top &amp;&amp; right){
                shaderMap[col+1,row-1].left++;
                shaderMap[col+1,row-1].bottom++;                
                shaderMap[col+1,row-1].inner++;
            }
            if(top_left &amp;&amp; top &amp;&amp; left){
                shaderMap[col-1,row-1].right++;
                shaderMap[col-1,row-1].bottom++;
                shaderMap[col-1,row-1].inner++;
            }
            if(bottom_right &amp;&amp; bottom &amp;&amp; right){
                shaderMap[col+1,row+1].left++;
                shaderMap[col+1,row+1].top++;
                shaderMap[col+1,row+1].inner++;
            }
            if(bottom_left &amp;&amp; bottom &amp;&amp; left){
                shaderMap[col-1,row+1].right++;
                shaderMap[col-1,row+1].top++;
                shaderMap[col-1,row+1].inner++;
            }   
            
            //TODO Tjuctions kinda nope they dont work 
                
                
        }
        
    }
}

